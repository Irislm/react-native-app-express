{
  "extends": "airbnb",
  "plugins": [
    "react",
    "react-native"
  ],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": 0,
    "react/forbid-prop-types": 0,
    "react/require-default-props": 0,
    "react/prop-types": 0,
    "react/destructuring-assignment": 0,
    "max-len": [1, 120],
    "no-plusplus": 0
  }
}
